import java.util.*;

class Volume {
    // Cube
    public double findVolume(double side) {
        return side * side * side;
    }

    // Cuboid
    public double findVolume(double length, double breadth, double height) {
        return length * breadth * height;
    }

    // Cylinder
    public double findVolume(float radius, float height) {
        return 3.14 * radius * radius * height;
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String shape = sc.nextLine().trim().toLowerCase();
        Volume v = new Volume();

        double volume = 0.0;

        switch (shape) {
            case "cube":
                double side = sc.nextDouble();
                volume = v.findVolume(side);
                break;

            case "cuboid":
                double l = sc.nextDouble();
                double b = sc.nextDouble();
                double h = sc.nextDouble();
                volume = v.findVolume(l, b, h);
                break;

            case "cylinder":
                float r = sc.nextFloat();
                float ht = sc.nextFloat();
                volume = v.findVolume(r, ht);
                break;
        }

        System.out.printf("%.2f", volume);
    }
}
