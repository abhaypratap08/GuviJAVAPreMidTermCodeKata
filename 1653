import java.util.Scanner;

abstract class Insurance {
    protected int basePremium;

    public Insurance(int basePremium) {
        this.basePremium = basePremium;
    }

    abstract double calculatePremium();
}

class LifeInsurance extends Insurance {
    public LifeInsurance(int basePremium) {
        super(basePremium);
    }

    @Override
    double calculatePremium() {
        return basePremium * 1.10;
    }
}

class HealthInsurance extends Insurance {
    public HealthInsurance(int basePremium) {
        super(basePremium);
    }

    @Override
    double calculatePremium() {
        return basePremium * 1.20;
    }
}

class CarInsurance extends Insurance {
    public CarInsurance(int basePremium) {
        super(basePremium);
    }

    @Override
    double calculatePremium() {
        return basePremium * 1.30;
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String insuranceType = sc.next().toLowerCase();
        int basePremium = sc.nextInt();

        Insurance insurance = null;
        String displayType = "";

        if (insuranceType.equals("life")) {
            insurance = new LifeInsurance(basePremium);
            displayType = "Life Insurance";
        } else if (insuranceType.equals("health")) {
            insurance = new HealthInsurance(basePremium);
            displayType = "Health Insurance";
        } else if (insuranceType.equals("car")) {
            insurance = new CarInsurance(basePremium);
            displayType = "Car Insurance";
        } else {
            System.out.println("Invalid insurance type entered.");
            sc.close();
            return;
        }

        double finalPremium = insurance.calculatePremium();

        System.out.println("Insurance Type: " + displayType);
        System.out.println("Premium: " + finalPremium);

        sc.close();
    }
}
