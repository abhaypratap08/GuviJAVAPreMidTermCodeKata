import java.util.Scanner;
interface AudioPlayer {
    void playAudio();
}
interface VideoPlayer {
    void playVideo();
}
class MediaCenter implements AudioPlayer, VideoPlayer {
    private String fileName;
    public MediaCenter(String fileName) {
        this.fileName = fileName;
    }
    public void playAudio() {System.out.println("Audio file detected: Playing with 128kbps quality");}
    public void playVideo() {System.out.println("Video file detected: Playing at 1080p resolution");}
    public void detectAndPlay() {
        String extension = fileName.substring(fileName.lastIndexOf('.') + 1).toLowerCase();
        if (extension.equals("mp3") || extension.equals("wav") || extension.equals("flac") || extension.equals("aac")) {
            playAudio();
        } else if (extension.equals("mp4") || extension.equals("avi") || extension.equals("mkv") || extension.equals("mov")) {
            playVideo();
        }
    }
}
class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        String fileName = s.nextLine();
        MediaCenter mediaCenter = new MediaCenter(fileName);
        mediaCenter.detectAndPlay();
        s.close();
    }
}
