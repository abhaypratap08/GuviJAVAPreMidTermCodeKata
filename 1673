import java.util.*;

class SumOfSquares<T extends Number> {
    public double computeSumOfSquares(List<T> list) {
        double sum = 0.0;
        for (T num : list) {
            double val = num.doubleValue();
            sum += val * val;
        }
        return sum;
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        List<Double> numbers = new ArrayList<>();
        for (int i = 0; i < n; i++) {
            numbers.add(sc.nextDouble());
        }

        SumOfSquares<Double> sos = new SumOfSquares<>();
        double sumSquares = sos.computeSumOfSquares(numbers);

        System.out.printf("Sum of Squares: %.1f", sumSquares);
    }
}
